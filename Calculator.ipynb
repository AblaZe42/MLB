   "source": [
    "import numpy as np\n",
    "from scipy.optimize import fsolve\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "delta = 0.01\n",
    "d = 11"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": []
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "def equation(n):\n",
    "    return np.sqrt((8*np.log((2*((2*n)**d+1))/delta))/n) - 0.01"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "15609625.408065483\n"
     ]
    }
   ],
   "source": [
    "n_val = fsolve(equation, 1000)\n",
    "print(n_val[0])"
   ]
  },
